                <TextBlock TextWrapping="Wrap" Text="i" />
            </Button.Content>
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="40"/>
                </Style>
            </Button.Resources>
        </Button>


        <!--Version-->
        <TextBlock Grid.Row="6"
                   Grid.Column="6"
                   Foreground="Gold" 
                   FontSize="34"
                   FontWeight="Bold"
                   Text="{Binding EditorVersion, UpdateSourceTrigger=PropertyChanged}"
                   Margin="110,5,0,0"/>
        <!--Discord-->
        <TextBlock
            Grid.Row="6"
            Grid.Column="6"
            Margin="60,45,0,80"
            FontSize="25">
        <Text:Hyperlink
            NavigateUri="{Binding DiscordServer, UpdateSourceTrigger=PropertyChanged}"
            RequestNavigate="Hyperlink_RequestNavigate">
            Discord Server
        </Text:Hyperlink>
        </TextBlock>
        <!--Youtube-->
        <TextBlock
            Grid.Row="6"
            Grid.Column="6"
            Margin="55,80,0,40"
            FontSize="25">
        <Text:Hyperlink
            NavigateUri="{Binding YoutubeLink, UpdateSourceTrigger=PropertyChanged}"
            RequestNavigate="Hyperlink_RequestNavigate">
            Youtube Playlist
        </Text:Hyperlink>
        </TextBlock>
        <!--Youtube-->
        <TextBlock
            Grid.Row="6"
            Grid.Column="6"
            Margin="100,115,0,10"
            FontSize="25">
        <Text:Hyperlink
            NavigateUri="{Binding TutorialLink, UpdateSourceTrigger=PropertyChanged}"
            RequestNavigate="Hyperlink_RequestNavigate">
            Tutorial
        </Text:Hyperlink>
        </TextBlock>


        <Image Source="{Binding SnowImg2, UpdateSourceTrigger=PropertyChanged}"
               Grid.RowSpan="3"
               Grid.Column="4"
               Grid.ColumnSpan="2"
               Height="440"
               Margin="135,14,2,9"/>

    </Grid>


</UserControl>
