<UserControl x:Class="UR_pnach_editor.Views.TextureView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UR_pnach_editor.Views" xmlns:viewmodels="clr-namespace:UR_pnach_editor.ViewModels"
             mc:Ignorable="d"
             d:DesignHeight="1080" d:DesignWidth="1920"
             KeyDown="Window_KeyDown"
             Focusable="True"
             Loaded="Window_Loaded">



    <Grid Height="1080" Width="1920">

        <Grid.Background>
            <ImageBrush ImageSource="/Resources/background.png"/>
        </Grid.Background>

        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
            <RowDefinition Height="45"/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="450*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="450*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="450*"/>
            <ColumnDefinition Width="100*"/>
            <ColumnDefinition Width="400*"/>
        </Grid.ColumnDefinitions>


        <Image x:Name="gifImage"
               Width="1920"
               Height="1080"
               Grid.ColumnSpan="100"
               Grid.RowSpan="100"/>

        <MediaElement x:Name="customSound" LoadedBehavior="Play"/>



        <!--Base Folder Path-->
        <TextBox x:Name="BaseFolderPath"
                 Grid.Column="7"
                 Grid.Row="0"
                 Grid.RowSpan="2"
                 FontSize="22"
                 Text="{Binding BaseFolderPath, UpdateSourceTrigger=PropertyChanged}"
                 MaxLength="200"
                 Margin="20,10,20,30"/>
        <!--versionText-->
        <TextBlock Grid.Row="1"
                   Grid.Column="7"
                   Grid.RowSpan="2"
                   Foreground="Red" 
                   FontSize="36"
                   FontWeight="Bold"
                   FontFamily="Times New Roman"
                   Text="Base Folder Path"
                   Margin="65,20,50,20"/>
        <!--Save Path-->
        <Button Grid.Row="3"
                Grid.Column="7"
                x:Name="BaseFolderBTN"
                Background="#AAAAAA"
                FontFamily="Times New Roman"
                FontSize="35"
                FontStyle="Italic"
                FontWeight="Bold"
                Margin="30,0,30,0"
                Click="BaseFolder_Click">
            Save Base Path
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="40"/>
                </Style>
            </Button.Resources>
        </Button>

        <!--Dump Folder Path-->
        <TextBox x:Name="DumpFolderPath"
                 Grid.Column="7"
                 Grid.Row="5"
                 Grid.RowSpan="2"
                 FontSize="22"
                 Text="{Binding DumpFolderPath, UpdateSourceTrigger=PropertyChanged}"
                 MaxLength="200"
                 Margin="20,10,20,30"/>
        <!--versionText-->
        <TextBlock Grid.Row="6"
                   Grid.RowSpan="2"
                   Grid.Column="7"
                   Foreground="#00CC00" 
                   FontSize="33"
                   FontWeight="Bold"
                   FontFamily="Times New Roman"
                   Text="Replacements Folder Path"
                   Margin="5,25,5,20"/>
        <!--Save Path-->
        <Button Grid.Row="8"
                Grid.Column="7"
                x:Name="DumpFolderBTN"
                Background="#AAAAAA"
                FontFamily="Times New Roman"
                FontSize="36"
                FontStyle="Italic"
                FontWeight="Bold"
                Margin="30,0,30,0"
                Click="DumpFolder_Click">
            Save Repl. Path
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="40"/>
                </Style>
            </Button.Resources>
        </Button>


